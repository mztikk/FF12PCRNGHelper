<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <StartupObject>FF12PCRNGHelper.Program</StartupObject>
    <ApplicationIcon>Resources\icon.ico</ApplicationIcon>
    <PlatformTarget>x64</PlatformTarget>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <Platforms>x64</Platforms>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\injection.png" />
    <None Remove="Resources\search.png" />
    <None Remove="Resources\settings.png" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Resources\icon.ico" />
    <Content Include="Resources\injection.png" />
    <Content Include="Resources\search.png" />
    <Content Include="Resources\settings.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="RFReborn" Version="1.6.0" />
    <PackageReference Include="RFReborn.Windows" Version="1.6.0" />
  </ItemGroup>
</Project>